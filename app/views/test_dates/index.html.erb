<h1>Listing test_dates</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Deadline</th>
      <th>Testing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_dates.each do |test_date| %>
      <tr>
        <td><%= test_date.date %></td>
        <td><%= test_date.deadline %></td>
        <td><%= test_date.testing %></td>
        <td><%= link_to 'Show', test_date %></td>
        <td><%= link_to 'Edit', edit_test_date_path(test_date) %></td>
        <td><%= link_to 'Destroy', test_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test date', new_test_date_path %>
